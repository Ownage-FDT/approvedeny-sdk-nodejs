{
  "name": "@ownage/approvedeny",
  "version": "1.0.0",
  "description": "A Node.js SDK for the Approvedeny API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist", 
    "LICENSE.md",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=16"
  },
  "private": "false",
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "build": "tsc",
    "test": "jest",
    "prepublish": "npm run build",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ownage-FDT/approvedeny-sdk-nodejs.git"
  },
  "keywords": [
    "approvedeny",
    "approvedeny-sdk",
    "approvedeny-sdk-nodejs"
  ],
  "author": "Olatayo Olayemi <olatayo.olayemi.peter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ownage-FDT/approvedeny-sdk-nodejs/issues"
  },
  "homepage": "https://github.com/Ownage-FDT/approvedeny-sdk-nodejs#readme",
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "nock": "^13.3.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": "eslint --cache --fix && prettier --write --ignore-unknown"
  }
}
